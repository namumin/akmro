	</servlet-mapping>
	<servlet>
		<description>Ajax client Listener</description>
		<display-name>AjaxListenerServlet</display-name>
		<servlet-name>AjaxListenerServlet</servlet-name>
		<servlet-class>wdf.servlets.AjaxListenerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FCHttpServlet</servlet-name>
		<url-pattern>*.do</url-pattern>
		<url-pattern>*.json</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AjaxListenerServlet</servlet-name>
		<url-pattern>*.lsnr</url-pattern>
	</servlet-mapping>
	<mime-mapping>
		<extension>htc</extension>
		<mime-type>text/plain</mime-type>
	</mime-mapping>
